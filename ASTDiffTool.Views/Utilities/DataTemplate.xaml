<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:ASTDiffTool.ViewModels;assembly=ASTDiffTool.ViewModels"
                    xmlns:view="clr-namespace:ASTDiffTool.Views">
    
    <DataTemplate DataType="{x:Type vm:ProjectPageViewModel}">
        <view:ProjectPage />
    </DataTemplate>    
    
    <DataTemplate DataType="{x:Type vm:NewProjectPageViewModel}">
        <view:NewProjectPage />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TreeDisplayPageViewModel}">
        <view:TreeDisplayPage />
    </DataTemplate>
    
</ResourceDictionary>